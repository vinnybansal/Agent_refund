
   <% form_for @user do |form| %>  
    <%= form.error_messages %>
    <div class="formList">
    <table>
    <tr><td>
    <%= form.label :login, "Login" %>
    </td><td>
    <%= form.text_field :login %>
    </td></tr>
    <tr><td>
    <%= form.label :email, "Email" %>
    </td><td>
    <%= form.text_field :email %>
    </td></tr>
    <tr><td>
    <%= form.label :password, "Password" %>
    </td><td>
    <%= form.password_field :password %>
    </td></tr>
    <tr><td>
    <%= form.label :password_confirmation, "Password confirmation" %>
    </td><td>
    <%= form.password_field :password_confirmation %>
    </td></tr>
    <tr><td>
    <%= form.label :first_name, "First Name" %>
    </td><td>
    <%= form.text_field :first_name %>
    </td></tr>
    
    <tr><td>
    <%= form.label :last_name, "Last Name" %>
    </td><td>
    <%= form.text_field :last_name %>
    </td></tr>
    
    <tr><td>
    <%= form.label :City, "City" %>
    </td><td>
    <%= form.text_field :city %>
    </td></tr>
    <tr><td>
    <%= form.label :zip, "Zip" %>
    </td><td>
    <%= form.text_field :zip %>
    </td></tr>

    <%= form.hidden_field :user_type, :value => "seller" %>
    <tr><td>
    <%= form.label :property_price, "PropertyPrice" %>
    </td><td>
    <%= form.text_field :property_price %>
    </td></tr>
   <tr><td>
    <%= form.label :property_address, "PropertyAdress" %>
    </td><td>
    <%= form.text_area  :property_address %>
    </td></tr>
   
    <tr><td>
    <%= form.label :reason_for_selling, "Reasonforselling" %>
    </td><td>
    <%= form.text_area  :reason_for_selling %>
    </td></tr>

    <tr><td>
    <%= form.submit "Submit" %>
    </td></tr></table>
<% end %> 
